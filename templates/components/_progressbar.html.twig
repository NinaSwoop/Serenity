<div class="progress rounded-4 {{category.progressColor}} m-3">
	<div class="progress-bar rounded-4-start {{ category.progressbarColor }}" role="progressbar" aria-label="Example with label" style="width: 40%;" aria-valuenow="4" aria-valuemin="0" aria-valuemax="10">
		<h4 class="progress-title px-3">{{category.title}}</h4>
	</div>
	<span class="progress-level align-self-center ms-auto p-2 fs-4">4/10</span>
</div>
